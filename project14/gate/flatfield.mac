/control/execute verbose.mac #make sure folder is correct

/gate/verbose Physic    0
/gate/verbose Cuts      0
/gate/verbose SD        0
/gate/verbose Actions   0
/gate/verbose Actor     0
/gate/verbose Step      0
/gate/verbose Error     0
/gate/verbose Warning   0
/gate/verbose Output    0
/gate/verbose Beam      0
/gate/verbose Volume    0
/gate/verbose Image     0
/gate/verbose Geometry  0
/tracking/verbose	0

#=====================================================
# GEOMETRY
#=====================================================

/control/useDoublePrecision # avoid rounding

/gate/geometry/setMaterialDatabase ../../../gate_v8.0/GateMaterials.db

# World
/gate/world/geometry/setXLength 1. m
/gate/world/geometry/setYLength 1. m
/gate/world/geometry/setZLength 6. m
/gate/world/setMaterial Air
/gate/world/vis/forceWireframe
/gate/world/vis/setColor black

# CTscanner system
/gate/world/daughters/name CTscanner
/gate/world/daughters/insert box
/gate/CTscanner/geometry/setXLength  15.15 mm
/gate/CTscanner/geometry/setYLength  0.150 mm
/gate/CTscanner/geometry/setZLength  6.0 mm
/gate/CTscanner/placement/setTranslation 0. 0. -4.150 mm
/gate/CTscanner/setMaterial Air
/gate/CTscanner/vis/forceWireframe
/gate/CTscanner/vis/setColor white

# Module
/gate/CTscanner/daughters/name module
/gate/CTscanner/daughters/insert box
/gate/module/geometry/setXLength  15.15 mm
/gate/module/geometry/setYLength  0.150 mm
/gate/module/geometry/setZLength  6.0 mm
/gate/module/setMaterial Air
/gate/module/vis/forceWireframe
/gate/module/vis/setColor white

# Cluster
/gate/module/daughters/name cluster
/gate/module/daughters/insert box
/gate/cluster/geometry/setXLength  15.15 mm
/gate/cluster/geometry/setYLength  0.150 mm
/gate/cluster/geometry/setZLength  6.0 mm
/gate/cluster/setMaterial Air
/gate/cluster/vis/forceWireframe
/gate/cluster/vis/setColor white

# Pixel
/gate/cluster/daughters/name pixel
/gate/cluster/daughters/insert box
/gate/pixel/geometry/setXLength 0.150 mm
/gate/pixel/geometry/setYLength 0.150 mm
/gate/pixel/geometry/setZLength 6.0 mm
/gate/pixel/setMaterial CsI
/gate/pixel/vis/forceSolid
/gate/pixel/vis/setColor red
/gate/pixel/repeaters/insert cubicArray
/gate/pixel/cubicArray/setRepeatNumberX 101
/gate/pixel/cubicArray/setRepeatNumberY 1
/gate/pixel/cubicArray/setRepeatNumberZ   1
/gate/pixel/cubicArray/setRepeatVector 0.150 0. 0. mm

/gate/systems/CTscanner/module/attach module
/gate/systems/CTscanner/cluster_0/attach cluster
/gate/systems/CTscanner/pixel_0/attach pixel

/gate/pixel/attachCrystalSD

# Phantom

# virtual sample mask #
/gate/world/daughters/name virtualvolume
/gate/world/daughters/insert box
/gate/virtualvolume/geometry/setXLength 20.0 mm
/gate/virtualvolume/geometry/setYLength 2.0 mm
/gate/virtualvolume/geometry/setZLength 0.700 mm
/gate/virtualvolume/setMaterial Air
/gate/virtualvolume/vis/forceWireframe
/gate/virtualvolume/vis/setColor white
/gate/virtualvolume/placement/setTranslation  0.0 0.0 600. mm



