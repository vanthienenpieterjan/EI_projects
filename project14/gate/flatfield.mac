/gate/world/daughters/insert box
/gate/plasticLayer/placement/setTranslation  0.0 0.0 -1.025 mm
/gate/plasticLayer/geometry/setXLength  150.0 mm
/gate/plasticLayer/geometry/setYLength  0.150 mm
/gate/plasticLayer/geometry/setZLength  0.250 mm
/gate/plasticLayer/setMaterial Plastic
/gate/plasticLayer/vis/forceWireframe
/gate/plasticLayer/vis/setColor gray

# carbon layer #
/gate/world/daughters/name carbonLayer
/gate/world/daughters/insert box
/gate/carbonLayer/placement/setTranslation  0.0 0.0 -0.525 mm
/gate/carbonLayer/geometry/setXLength  150.0 mm
/gate/carbonLayer/geometry/setYLength  0.150 mm
/gate/carbonLayer/geometry/setZLength  0.750 mm
/gate/carbonLayer/setMaterial Carbon
/gate/carbonLayer/vis/forceWireframe
/gate/carbonLayer/vis/setColor gray

/gate/virtualvolume/attachPhantomSD

#/gate/BerylliumCylinder1/attachPhantomSD
/gate/plasticLayer/attachPhantomSD
/gate/carbonLayer/attachPhantomSD

